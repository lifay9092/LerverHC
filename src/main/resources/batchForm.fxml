<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="rootPane" prefHeight="234.0" prefWidth="503.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cn.lifay.lerverhc.view.BatchFormController">
   <children>
      <Button fx:id="btnDataFile" layoutX="35.0" layoutY="90.0" mnemonicParsing="false" onAction="#selectDataFile" prefHeight="38.0" prefWidth="109.0" style="-fx-text-fill:WHITE;-fx-background-color:#596bb4" text="选择数据文件" />
      <TextArea fx:id="batchDataFilePath" layoutX="151.0" layoutY="83.0" prefHeight="51.0" prefWidth="311.0" />
      <Button layoutX="35.0" layoutY="153.0" mnemonicParsing="false" onAction="#checkDataFile" prefHeight="23.0" prefWidth="109.0" style="-fx-text-fill:WHITE;-fx-background-color:#744ead" text="检查数据" />
      <TextField fx:id="checkResult" editable="false" layoutX="151.0" layoutY="153.0" prefHeight="23.0" prefWidth="311.0" text="检查结果" />
      <Label fx:id="batchFileNameLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="35.0" layoutY="44.0" prefHeight="23.0" prefWidth="116.0" text="输出文件名[变量]：" textFill="#097ee4" underline="true">
         <font>
            <Font name="Microsoft YaHei Bold" size="13.0" />
         </font></Label>
      <TextField fx:id="batchFileNameText" layoutX="151.0" layoutY="44.0" prefHeight="23.0" prefWidth="156.0" />
      <Label fx:id="batchFileExtLabel" layoutX="314.0" layoutY="46.0" prefHeight="19.0" prefWidth="68.0" text=".json" />
      <Label fx:id="help" alignment="CENTER" contentDisplay="CENTER" layoutX="198.0" layoutY="14.0" prefHeight="23.0" prefWidth="116.0" text="友情提示" textFill="#097ee4" underline="true">
         <font>
            <Font name="Microsoft YaHei Bold" size="13.0" />
         </font>
      </Label>

   </children>
</AnchorPane>
